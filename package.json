{
  "name": "nitro-test-utils",
  "type": "module",
  "version": "0.10.0",
  "packageManager": "pnpm@10.20.0",
  "description": "Testing environment and utilities for Nitro",
  "author": "Johann Schopplich <hello@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/nitro-test-utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/nitro-test-utils.git"
  },
  "bugs": {
    "url": "https://github.com/johannschopplich/nitro-test-utils/issues"
  },
  "keywords": [
    "nitro",
    "nitropack",
    "nuxt"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "default": "./dist/config.js"
    },
    "./e2e": {
      "types": "./dist/e2e.d.ts",
      "default": "./dist/e2e.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:types": "tsc --noEmit",
    "release": "bumpp",
    "prepare": "pnpm run build && pnpm -C test/fixture prepare && pnpm -C test/fixture2 prepare"
  },
  "peerDependencies": {
    "nitropack": "^2",
    "vitest": "^2 || ^3 || ^4"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "listhen": "^1.9.0",
    "ofetch": "^1.5.0",
    "pathe": "^2.0.3",
    "ufo": "^1.6.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.1.0",
    "@types/node": "^24.9.2",
    "bumpp": "^10.3.1",
    "eslint": "^9.38.0",
    "nitropack": "^2.12.9",
    "tsdown": "^0.15.12",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "vite": "^7.1.12",
    "vitest": "^4.0.5"
  }
}
