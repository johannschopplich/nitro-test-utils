{
  "name": "nitro-test-utils",
  "type": "module",
  "version": "0.8.0",
  "packageManager": "pnpm@9.9.0",
  "description": "Testing environment and utilities for Nitro",
  "author": "Johann Schopplich <hello@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/nitro-test-utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/nitro-test-utils.git"
  },
  "bugs": {
    "url": "https://github.com/johannschopplich/nitro-test-utils/issues"
  },
  "keywords": [
    "nitro",
    "nitropack",
    "nuxt"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./config": {
      "types": "./dist/config.d.mts",
      "import": "./dist/config.mjs"
    },
    "./e2e": {
      "types": "./dist/e2e.d.mts",
      "import": "./dist/e2e.mjs"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:types": "tsc --noEmit",
    "release": "bumpp",
    "prepare": "pnpm run build && pnpm -C test/fixture prepare && pnpm -C test/fixture2 prepare"
  },
  "peerDependencies": {
    "nitropack": "^2",
    "vitest": "^2"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "listhen": "^1.7.2",
    "ofetch": "^1.3.4",
    "pathe": "^1.1.2",
    "ufo": "^1.5.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.3",
    "@types/node": "^20.16.2",
    "bumpp": "^9.5.2",
    "eslint": "^9.9.1",
    "nitropack": "^2.9.7",
    "typescript": "^5.5.4",
    "unbuild": "^3.0.0-rc.7",
    "vite": "^5.4.2",
    "vitest": "^2.0.5"
  }
}
